<div>
  <h1>Carrito</h1>
</div>

<div>
  {{#each cart.products}}
    <div>
      <img
        src="{{product.thumbnails}}"
        alt="{{product.title}}"
        style="aspect-ratio: 1; width: 200px; object-fit: cover;"
      />

      <h2>Nombre: {{product.title}}</h2>
      <h3>Precio: ${{product.price}}</h3>
      <p>Cantidad: {{quantity}}</p>
    </div>
  {{/each}}
</div>